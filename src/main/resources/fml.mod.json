{
  "schemaVersion": 1,
  "id": "ga-higher",
  "version": "${version}",
  "depends": {
    "fishmodloader": ">=3.2.3",
    "rusted_iron_core": ">=1.3.7",
    "many-lib": ">=2.0.0"
  },
  "suggests": {
    "mite_ite": ">=2.0.9",
    "baubles": ">=1.0.9"
  },
  "environment": "*",
  "name": "GA-Higher",
  "description": "这是'新娱乐版'在FML3的移植. 由Debris完成移植工作, 并增添了若干新内容",
  "icon": "ga_bag.png",
  "authors": [
    "YueZhuXue",
    "Wensc",
    "Debris"
  ],
  "contributor": [
    "QF_Lose"
  ],
  "entrypoints": {
    "main": [
      "com.github.Debris.GAHigher.GAStart"
    ],
    "preLaunch": [
      "com.github.Debris.GAHigher.EarlyRiser"
    ],
    "modmenu": [
      "com.github.Debris.GAHigher.compat.ModMenuImpl"
    ],
    "emi": [
      "com.github.Debris.GAHigher.compat.EmiPlugin"
    ],
    "baubles": [
      "com.github.Debris.GAHigher.compat.BaublesPlugin"
    ]
  },
  "mixins": [
    "GAHigher.mixins.json"
  ],
  "accessWidener": "GAHigher.accesswidener",
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/ItemStack": [
        "com/github/Debris/GAHigher/api/GAItemStack"
      ]
    }
  }
}